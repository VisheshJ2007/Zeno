{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/index.py" },
    { "src": "^/$", "dest": "/frontend/Zeno_UI/zeno.html" },
    { "src": "^(.*)$", "dest": "/frontend/Zeno_UI$1" }
  ]
}